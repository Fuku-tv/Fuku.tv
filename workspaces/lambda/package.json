{
  "name": "fuku.tv-lambda",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "module": "index.ts",
  "scripts": {
    "clean": "rimraf .build .serverless .dynamodb .webpack",
    "build": "cross-env SLS_DEBUG=* sls package --stage local",
    "dev": "cross-env SLS_DEBUG=* sls offline --stage local --printOutput",
    "deploy": "sls deploy --stage local",
    "destroy": "sls remove --stage local"
  },
  "devDependencies": {
    "@types/auth0": "^2.33.3",
    "@types/aws-lambda": "^8.10.73",
    "@types/node": "^14.14.37",
    "@types/node-fetch": "^2.5.9",
    "@types/serverless": "^1.78.24",
    "serverless": "^2.33.1",
    "serverless-bundle": "^4.2.2",
    "serverless-dynamodb-local": "^0.2.39",
    "serverless-offline": "^6.9.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "agcod": "^1.0.4",
    "auth0": "^2.34.1",
    "aws-sdk": "^2.879.0",
    "node-fetch": "^2.6.1",
    "stripe": "^8.142.0"
  },
  "peerDependencies": {
    "fuku.tv-shared": "*"
  }
}
