{
  "name": "fuku.tv-client",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "parcel src/index.html --open --https",
    "dev": "yarn start",
    "postbuild": "ts-node ./scripts/sitemap.ts",
    "build": "parcel build src/index.html --public-url ./ ",
    "clean": "rimraf dist .cache .parcel-cache",
    "lint": "tsc && eslint . --ext .ts,.tsx --fix"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.4.0",
    "@reduxjs/toolkit": "^1.5.1",
    "@stripe/react-stripe-js": "^1.2.2",
    "@stripe/stripe-js": "^1.13.2",
    "@types/react-redux": "^7.1.12",
    "@types/redux": "^3.6.0",
    "axios": "^0.21.1",
    "canvas-confetti": "^1.3.2",
    "hamburger-react": "^2.4.0",
    "react": "^17.0.0",
    "react-awesome-button": "^6.5.1",
    "react-circular-progressbar": "^2.0.4",
    "react-cookie-consent": "^6.2.3",
    "react-countdown-circle-timer": "^2.5.1",
    "react-device-detect": "^1.15.0",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-sitemap": "^1.2.0",
    "react-tooltip": "^4.2.17",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "stripe": "^8.142.0"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "2.0.0-beta.2",
    "@parcel/transformer-sass": "2.0.0-beta.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "autoprefixer": "^10.2.5",
    "parcel": "^2.0.0-beta.2",
    "parcel-config-pwa-manifest": "^0.0.9",
    "parcel-reporter-static-files-copy": "^1.2.2",
    "postcss": "^8.1.0",
    "sass": "^1.30.0",
    "typescript": "^4.1.2",
    "typescript-plugin-css-modules": "^3.2.0"
  },
  "peerDependencies": {
    "fuku.tv-shared": "*",
    "h264-live-player": "*"
  },
  "alias": {
    "src": "./src"
  },
  "pwaManifest": {
    "name": "Fuku.tv",
    "shortName": "Fuku.tv",
    "startURL": "/",
    "theme": "#000000",
    "generateIconOptions": {
      "baseIcon": "./static/fuku.png",
      "genFavicons": true
    }
  },
  "staticFiles": {
    "staticPath": "static",
    "watcherGlob": "**"
  }
}
